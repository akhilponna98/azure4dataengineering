{
	"name": "Movies Cleanup Data Flow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "InputCSV",
						"type": "DatasetReference"
					},
					"name": "Movies"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "ExtractingYear"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movieId as string,",
				"          title as string,",
				"          genres as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Movies",
				"Movies derive(Year = right(title, 6),",
				"          title = left(title, length(title)-6)) ~> ExtractingYear"
			]
		}
	}
}